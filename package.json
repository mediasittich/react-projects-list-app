{
  "name": "react-express-starter",
  "version": "1.0.0",
  "description": "Boilerplate for React and Express",
  "main": "server.js",
  "scripts": {
    "client-install": "cd client && npm install",
    "start": "node ./server/server.js",
    "server": "nodemon ./server/server.js",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\""
  },
  "author": "Regina Galambos",
  "license": "MIT",
  "dependencies": {
    "body-parser": "^1.18.2",
    "concurrently": "^3.5.1",
    "express": "^4.16.3",
    "mongoose": "^5.0.13",
    "react-modal": "^3.3.2"
  },
  "devDependencies": {
    "nodemon": "^1.17.3",
    "webpack": "^4.5.0",
    "webpack-cli": "^2.0.14"
  }
}
